<!-- Header -->
<header class="masthead text-white text-center">
  <div class="container">
    <h1>Listing simulation entries</h1>

    <%= line_chart [["Nsm Og Milk", 28], ["Og Bananas", 19], ["Rr Cheese Pizza", 19], ["Casa Solar Tmprn", 10], ["Og Carrots", 9], ["Olive Bar", 8], ["Atlas Blkbry", 8], ["Mixed Greens Og", 7], ["Tangerine 3 Lb", 7], ["De Lallo Pasta", 7]],
      title: "Top Ten Products (By Total Quantity Purchased)",
      colors: ['brown'],
      library: {
        tooltip: {
          pointFormat: 'Times Bought: <b>{point.y}</b>'
        }
      }
    %>

      <table>
      <tr>
        <th>Gas Pedal Position</th>
        <th>Brake Pedal Position</th>
      </tr>
    
      <% @simulation_entries.each do |simulation_entry| %>
        <tr>
          <td><%= simulation_entry.gas_pedal_position %></td>
          <td><%= simulation_entry.brake_pedal_position %></td>
        </tr>
      <% end %>
    </table>


  </div>

  <%= link_to 'Back', static_pages_panel_path %>
  <%= link_to 'Analysis', analysis_index_path %>
</header>